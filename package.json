{
  "name": "tetracoord",
  "version": "0.7.1",
  "description": "Tetracoordinates engine, calculator.",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "_test": "mocha --import=tsx",
    "test": "npm run _test -- test/*.ts",
    "testjs": "mocha test/*.js",
    "pretestjs": "npm run compile",
    "cli": "tsx src/cli/index.ts",
    "compile": "tsc --build tsconfig.json",
    "_pack": "npm pack --pack-destination dist",
    "_prepublish": "npm run compile && npm run _pack",
    "clean": "rm index.js; rm index.d.ts; rm index.js.map; rm src/**/*.js; rm src/**/*.d.ts; rm src/**/*.js.map; rm test/**/*.js; rm test/**/*.d.ts; rm test/**/*.js.map; rm tsconfig.tsbuildinfo"
  },
  "bin": {
    "tcoord": "src/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogallagher/tetracoord.git"
  },
  "keywords": [
    "tetracoordinates",
    "typescript",
    "geometry",
    "2d",
    "vector",
    "math",
    "number-system",
    "arithmetic",
    "space-filling-curve",
    "fractal",
    "sierpinski-triangle"
  ],
  "author": "Owen Gallagher",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/ogallagher/tetracoord/issues"
  },
  "homepage": "https://github.com/ogallagher/tetracoord#readme",
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.130",
    "@types/pino": "^7.0.4",
    "@types/yargs": "^17.0.33",
    "mocha": "^11.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "pino": "^10.0.0",
    "pts-math": "^0.12.9",
    "subscript": "^9.1.0"
  },
  "optionalDependencies": {
    "pino-pretty": "^13.1.2",
    "tsx": "^4.20.6",
    "yargs": "^18.0.0"
  }
}
